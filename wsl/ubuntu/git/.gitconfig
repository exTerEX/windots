[user]
	name = exTerEX
	email = 30706568+exTerEX@users.noreply.github.com
	singingkey = B0C04CF48CEB0CAB
[core]
	autocrlf = input
	editor = code
	fileMode = true
	hidedotfiles = false
	symlinks = true
[color]
	ui = auto
[color "branch"]
	current = yellow bold
	local = green bold
	remote = cyan bold
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "status"]
	added = green bold
	changed = yellow bold
	untracked = red bold
[status]
	submoduleSummary = true
[diff]
	submodule = log
[default]
	protocol = ssh
[gpg]
	program = gpg
[commit]
	gpgsign = true
[tag]
	forcesignannotated = true
[help]
	autocorrect = 1
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[submodule]
	recurse = true
